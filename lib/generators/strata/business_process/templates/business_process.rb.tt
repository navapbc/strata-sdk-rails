class <%= business_process_name %>BusinessProcess < Strata::BusinessProcess

  <% if case_name != business_process_name %>
  def self.case_class
    "<%= case_name %>".constantize
  end
  <% end %>

  applicant_task('submit_application')

  start_on_application_form_created('submit_application')

  transition('submit_application', '<%= application_form_name %>Submitted', 'example_1')
  transition('example_1', 'a_different_event', 'end')
end
